  <!-- push urdf to factory and spawn robot in gazebo -->
  <node name="spawn_cylHohl2" pkg="gazebo" type="spawn_model" args=" -param cylHohl2 -urdf -x -1.6 -y 0.6 -z 0.75  -R 0 -P 1.570796 -Y 0 -model cylHohl2" respawn="false" output="screen" />

  <!-- send urdf to param server -->
  <param name="box_small" textfile="$(find cob_gazebo_objects)/objects/box_small.urdf" />

  <!-- push urdf to factory and spawn robot in gazebo -->
  <node name="spawn_box_small" pkg="gazebo" type="spawn_model" args=" -param box_small -urdf -x -1.5 -y 0.36 -z 0.65  -R 0 -P 0 -Y 0 -model box_small" respawn="false" output="screen" />

  <!-- send urdf to param server -->
  <param name="box_flat" textfile="$(find cob_gazebo_objects)/objects/box_flat.urdf" />

  <!-- push urdf to factory and spawn robot in gazebo -->
  <node name="spawn_box_flat" pkg="gazebo" type="spawn_model" args=" -param box_flat -urdf -x -1.5 -y -0.5 -z 0.65  -R 0 -P 0 -Y 0 -model box_flat" respawn="false" output="screen" />

