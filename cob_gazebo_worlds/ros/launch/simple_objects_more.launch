  <node name="spawn_cylHohl" pkg="gazebo" type="spawn_model" args=" -param cylHohl -urdf -x -1.8 -y 0.1 -z 0.65  -R 0 -P 0 -Y 0 -model cylHohl" respawn="false" output="screen" />


  <!-- send urdf to param server -->
